// Inline components requires fixing up named references of repeated element's init code inside. For Rust/C++
// this requires SLINT_INLINING=true to achieve test coverage.


component Compo  {
    callback dontcrash();
    dontcrash() => {}
    VerticalLayout {
        if (true) : Rectangle {
            init => {
                dontcrash();
            }
        }
    }
}

export component TestCase {
    out property <bool> ok;
    out property <bool> test: l.preferred-width == 0px && self.ok;
    l := VerticalLayout {
        Compo {
            dontcrash => {
                root.ok = true;
            }
        }
    }
}

